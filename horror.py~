print ("Yuk main pilih pintu")
pilih=input("Pilih 1 atau 2 ?")
if pilih==1:
    print(" 1.Harta")
    print(" 2.Tahta")
    print(" 3.Wanita")
    pilih2=input("Pilih 1/2/3 ? ")
    if pilih2==1:
        print("Anda Kaya")
    elif pilih2==2:
        print("Anda Raja")
    else:
        print("Anda Menderita")
else:
    print(" 1.Homo")
    print(" 2.Banci")
    pilih3=input("Pilih 1/2 ? ")
    if pilih3==1:
        print("Anda dimakan")
    else:
        print("Anda ikutan")
        
